{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "google",
    "rules": {
        "require-jsdoc": ["error", {
        "require": {
            "FunctionDeclaration": false,
            "MethodDefinition": false,
            "ClassDeclaration": false
        }
        }],
        "max-len": ["error", 100],
        "error": {
            "properties": "never"
        }
    }
}